    "json-stable-stringify-without-jsonify": "^1.0.1",
    "zigbee-herdsman": "^0.13.48",
    "zigbee-herdsman-converters": "^14.0.8"
  },
  "devDependencies": {
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "homebridge": "^1.2.3",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
