    "json-stable-stringify-without-jsonify": "^1.0.1",
    "zigbee-herdsman": "^0.13.175",
    "zigbee-herdsman-converters": "^14.0.335",
    "ansi-regex": ">=5.0.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "homebridge": "^1.3.8",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
